{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Debug",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithProperty"
    ],
    "Properties": {
      "ApplicationName": "King Price API"
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:4002",
          "application": "King Price API"
        }
      },
      {
        "Name": "Debug"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Port=5432;Database=budgettracker;Username=postgres;Password=postgres;Include Error Detail=true"
  },
  "Auth": {
    "AccessTokenValidityMinutes": 60,
    "RefreshTokenValidityMinutes": 180
  },
  "AllowedHosts": "*"
}
